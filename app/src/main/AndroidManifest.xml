        <activity android:name=".CourseDescriptionHome.BcomDepartmentActivity" />
        <activity android:name=".CourseDescriptionHome.BcaDepartmentActivity" />
        <activity android:name=".CourseDescriptionHome.BaDepartmentActivity" />
        <activity android:name=".drawer_details.DeveloperActivity" />
        <activity android:name=".drawer_details.ContactUsActivity" />
        <activity android:name=".authentication.RegisterActivity" />
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".authentication.ForgetPasswordActivity" />
        <activity
            android:name=".MainActivity"
                   android:exported="true">
                <intent-filter>
                       <action android:name="android.intent.action.MAIN" />

                    <category android:name="android.intent.category.LAUNCHER" />
                </intent-filter>
        </activity>
    </application>

</manifest>