<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dpbs.collegeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/collegelogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollegeApp2">
        <activity android:name=".TermsConditionActivity" />
        <activity android:name=".PrivacyPolicyActivity" />
        <activity android:name=".ui.Videos.YoutubePlayerActivity" />
        <activity android:name=".FullImageView" />
        <activity android:name=".Pdf.PdfViewActivity" />
        <activity android:name=".Pdf.PdfActivity" />
